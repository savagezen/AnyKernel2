   write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_rate 50000
   write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_slack -1
   write /sys/devices/system/cpu/cpu0/cpufreq/interactive/hispeed_freq 384000
   write /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads "460000:92 600000:92 672000:90 768000:14 864000:90 960000:90 1248000:99 1344000:90 1478000:10 1555000:99"
