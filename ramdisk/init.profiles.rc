# SPECTRUM KERNEL MANAGER
# Ramdisk file for profile based kernel management
# Implimentation inspired by Franco's fku profiles

# Initialization
on property:sys.boot_completed=1
   # Set default profile on first boot
   # Executed under three different contexts to make sure it fires properly
   exec u:r:init:s0 root root -- /init.profiles.sh
   exec u:r:su:s0 root root -- /init.profiles.sh
   exec u:r:supersu:s0 root root -- /init.profiles.sh
   # Enable Spectrum support
   setprop spectrum.support 1
   # Set kernel name
   setprop persist.spectrum.kernel savagezen

# Balance v1 by savagezen
on property:persist.spectrum.profile=0
# Battery v1.2 by savagezen (based on Flash Kernel Battery)
on property:persist.spectrum.profile=1
   # i/o scheduler settings
   write /sys/block/mmcblk0/queue/scheduler noop
   write /sys/block/mmcblk0/queue/read_ahead_kb 5120
   # virtual memory settings
   sysctl -w vm.min_free_kbytes 24860
   sysctl -w vm.im_free_kbytes 43200

# Balance v1 by savagezen
on property:persist.spectrum.profile=0
   # I/O Scheduler Settings v1.0
   write /sys/block/mmcblk0/queue/scheduler noop
   write /sys/block/mmcblk0/queue/read_ahead_kb 5120
#v2.0   write /sys/block/mmcblk0/queue/scheduler fiops
#v2.0   write /sys/block/mmcblk0/queue/read_ahead_kb 512
   # Virtual Memory Settings
   sysctl -w vm.min_free_kbytes 24860
   sysctl -w vm.extra_free_kbytes 43200
   # Frequency values
   write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 384000
   write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1555000
   write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 384000
   write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 1958000
#v2.0      write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 633000
   # Force cores #6 and #7 offline
   write /sys/module/msm_performance/parameters/max_cpus "-1:2"
   # Little values
   write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor interactive
   write /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load 0
#v2.0   write /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load 72
